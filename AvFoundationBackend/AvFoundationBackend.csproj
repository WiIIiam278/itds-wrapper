        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\ITDSWrapper\ITDSWrapper.csproj" />
    </ItemGroup>

</Project>
